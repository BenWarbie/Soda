# Solana Configuration
SOLANA_NETWORK=devnet  # mainnet-beta, testnet, devnet
SOLANA_RPC_URL=https://api.devnet.solana.com

# Wallet Configuration
MAIN_WALLET_PRIVATE_KEY=  # Your main wallet private key
MAX_WALLETS=10           # Maximum number of trading wallets to create

# Trading Configuration
TRADING_MODE=SAFE        # SAFE, NORMAL, AGGRESSIVE, HIGH_FREQUENCY
MIN_SOL_PER_WALLET=0.1  # Minimum SOL to maintain in each wallet

# Risk Management
STOP_LOSS_THRESHOLD=0.95
TRAILING_STOP=false
TRAILING_DISTANCE=0.02

# WebSocket Configuration
WS_PORT=8000
FRONTEND_PORT=3000
